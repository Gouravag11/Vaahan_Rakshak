<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.VaahanRakshak" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.VaahanRakshak.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.VaahanRakshak.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.VaahanRakshak.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="sauber" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="backgroundTint">@color/dear_rosaline</item>
        <item name="colorPrimary">@color/umberwine</item>
        <item name="android:background">@color/dear_rosaline</item>
        <item name="android:backgroundTint">@color/dear_rosaline</item>
    </style>

    <style name="onSauber" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="backgroundTint">@color/umberwine</item>
        <item name="android:backgroundTint">@color/umberwine</item>
    </style>
</resources>